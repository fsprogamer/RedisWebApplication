{
 "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" },
      {
        "Name": "File",
        "Args": {
          "path": ".\\Logs\\redis_log.txt",
          "rollingInterval": "Day",
          "shared": true
        }
      }
    ],
  "Properties": {
    "Application": "sampleapp"
  } 
  },
  "Redis": {
    "Host": "localhost",
    "Port": "6379"
  },
  "AllowedHosts": "*"

}
